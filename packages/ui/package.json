        "types": "./build/index.d.ts",
        "default": "./build/index.js"
      }
    },
    "./styles.css": "./dist/styles.css",
    "./hero": "./hero.js"
  },
  "files": [
    "dist",
    "build",
    "hero.js",
    "LICENSE"
